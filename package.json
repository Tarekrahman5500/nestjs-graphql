{
  "name": "nestjs-graphql",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "bun run nest build",
    "format": "bun run prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "bun run nest start",
    "start:dev": "bun run nest start --watch",
    "start:debug": "bun run nest start --debug --watch",
    "start:prod": "bun run node dist/main.js",
    "lint": "bun run eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "bun run lint",
    "test:watch": "bun run jest --watch",
    "test:cov": "bun run jest --coverage",
    "test:debug": "bun run node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "bun run jest --config ./test/jest-e2e.json",
    "generate:types": "bun src/generate-types.ts",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:restart": "docker compose restart",
    "docker:logs": "docker compose logs -f",
    "typeorm": "bun run build && ts-node ./node_modules/typeorm/cli",
    "migration:run": "bun run build &&  bun typeorm migration:run -d dist/config/typeorm.js",
    "migration:generate": "bun run build &&  bun typeorm -d dist/config/typeorm.js migration:generate ./src/migrations/$orm",
    "migration:create": "bun typeorm migration:create ./src/migrations/$orm",
    "migration:revert": "bun run build && bun typeorm -d dist/config/typeorm.js migration:revert"
  },
  "dependencies": {
    "@faker-js/faker": "^9.9.0",
    "@nestjs/apollo": "^13.1.0",
    "@nestjs/common": "^11.1.3",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.3",
    "@nestjs/graphql": "^13.1.0",
    "@nestjs/platform-express": "^11.1.3",
    "@nestjs/typeorm": "^11.0.0",
    "@types/argon2": "^0.15.4",
    "argon2": "^0.43.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dataloader": "^2.2.3",
    "graphql": "^16.11.0",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "typeorm": "^0.3.25",
    "znv": "^0.5.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.30.0",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.3",
    "@swc/cli": "^0.7.7",
    "@swc/core": "^1.12.9",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.8",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.30.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "globals": "^16.3.0",
    "jest": "^30.0.3",
    "npm-check-updates": "^18.0.1",
    "prettier": "^3.6.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "trustedDependencies": [
    "@nestjs/core",
    "@swc/core"
  ]
}
